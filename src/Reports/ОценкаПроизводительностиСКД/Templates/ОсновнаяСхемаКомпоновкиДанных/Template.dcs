	КОГДА &amp;ПереодичностьЗамеров="День" ТОГДА ФОРМАТ(ПериодЗамера, "ДФ='дд.ММ.гггг'") + "г."
	КОГДА &amp;ПереодичностьЗамеров="Неделя" ТОГДА
		ВЫБОР
			КОГДА НАЧАЛОПЕРИОДА(ПериодЗамера, "НЕДЕЛЯ") &lt; &amp;НачалоПериода ТОГДА ФОРМАТ(&amp;НачалоПериода, "ДФ='дд.ММ.гггг''г.'''") + " - " + ФОРМАТ(КОНЕЦПЕРИОДА(ПериодЗамера,"НЕДЕЛЯ"), "ДФ='дд.ММ.гггг''г. '''")
			КОГДА КОНЕЦПЕРИОДА(ПериодЗамера, "НЕДЕЛЯ") &gt; &amp;КонецПериода ТОГДА ФОРМАТ(НАЧАЛОПЕРИОДА(ПериодЗамера, "НЕДЕЛЯ"), "ДФ='дд.ММ.гггг''г.'''")+ " - " + ФОРМАТ(&amp;КонецПериода, "ДФ='дд.ММ.гггг''г. '''")
			ИНАЧЕ ФОРМАТ(НАЧАЛОПЕРИОДА(ПериодЗамера, "НЕДЕЛЯ"), "ДФ='дд.ММ.гггг''г.'''")+ " - " + ФОРМАТ(КОНЕЦПЕРИОДА(ПериодЗамера,"НЕДЕЛЯ"), "ДФ='дд.ММ.гггг''г. '''")
		КОНЕЦ
	КОГДА &amp;ПереодичностьЗамеров="Весь период" ТОГДА ФОРМАТ(&amp;НачалоПериода, "ДФ='дд.ММ.гггг'") + " - " + ФОРМАТ(&amp;КонецПериода, "ДФ='дд.ММ.гггг'")
